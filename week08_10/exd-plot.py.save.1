#!/usr/bin/env python3

import sys

s = sys.stdin.readlines()

i = 0
pos = []
while i < len(s):
    pos.append(int(s[i].rstrip()))
    i += 1
#print(pos)

j = 0
i = 0
c = 0
plot = []
while i < len(pos):
    c = pos[0]
    pos.pop(0)
    while i > c + 1:
        if i == 0:
            plot.append('|')
        else:
            if i in pos:
                plot.append('*')
            else:
                plot.append('_')
#    if j not in pos:
#        plot.append(' ')
#    else:
#        plot.append('*')
    i += 1
